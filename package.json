{
  "name": "cjs-whatsapp",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "rm -rf build && pkg . --out-path build",
    "postbuild": "cp -R ./node_modules/puppeteer/.local-chromium build/chromium && cp data.xlsx build && cp -R images build"
  },
  "pkg": {
    "scripts": "*.js",
    "outputPath": "dist",
    "assets": [],
    "targets": [
      "node16-win-x64"
    ]
  },
  "dependencies": {
    "chalk": "4.1.2",
    "exceljs": "^4.3.0",
    "pkg": "^5.8.1",
    "qrcode-terminal": "^0.12.0",
    "readline": "^1.3.0",
    "whatsapp-web.js": "^1.19.5"
  }
}
